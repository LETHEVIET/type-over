{
  "name": "TypeOver",
  "description": "manifest.json description",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,css,scss,md,mdx,json,yml,yaml,html}\" --write",
    "format:check": "prettier \"**/*.{js,jsx,ts,tsx,css,scss,md,mdx,json,yml,yaml,html}\" --check",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-react": "^1.1.3",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "wxt": "^0.20.6"
  }
}
